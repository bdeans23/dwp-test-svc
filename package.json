{
  "name": "test-dwp-svc",
  "version": "0.1.0",
  "description": "Microservice to retrieve data from DWP API",
  "main": "server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "compliance:tslint": "tslint -c tslint.json -p tsconfig.json",
    "compliance:gpl": "exit $( yarn licenses list | grep -v 'LGPL' | grep -c 'GPL' )",
    "security:outdated-packages": "npm outdated",
    "security:vulnerable-packages": "nsp check",
    "test": "jest --verbose",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "docs": "jsdoc dist/**/*.js"
  },
  "nyc": {
    "per-file": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.js",
      "**/*.d.ts",
      "**/*.spec.ts",
      "dist/*.*",
      "dist/**/*.*",
      "build/*.*",
      "coverage/**/*.*",
      "src/server.ts",
      "src/app.ts",
      "src/config.ts"
    ],
    "all": true
  },
  "author": "Barry Deans",
  "license": "MIT",
  "dependencies": {
    "@types/pino": "^5.8.10",
    "@types/jest": "^24.0.19",
    "amqplib": "^0.5.5",
    "assert": "^1.5.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-boom": "^2.0.0",
    "helmet": "^3.21.0",
    "joi": "^13.7.0",
    "json-stringify-safe": "^5.0.0",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "pino": "^5.13.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rimraf": "^2.7.1",
    "superagent": "^3.8.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.1",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^0.0.37",
    "@types/joi": "^13.6.3",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "^2.2.48",
    "@types/morgan": "^1.7.37",
    "@types/node": "^9.6.52",
    "@types/proxyquire": "^1.3.28",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.44",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "^4.3.3",
    "@types/sinon-chai": "^2.7.36",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "express-validator": "^5.3.1",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "mocha": "^5.2.0",
    "nsp": "^3.2.1",
    "nyc": "^11.9.0",
    "proxyquire": "^2.1.3",
    "rewire": "^3.0.2",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.3.0",
    "snyk": "^1.226.1",
    "supertest": "^3.4.2",
    "ts-jest": "^22.0.4",
    "ts-node": "^5.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.5.1"
  }
}